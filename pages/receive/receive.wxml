<!--pages/receive.wxml-->
<view class="head">
  <i-icon type="return" size="28" color="white" bindtap='handleReturn'></i-icon>
  <text>{{taskName}}</text>
  <i-icon type="add" size="28" color="#77a9fd"></i-icon>
</view>

<view class=" page__bd page__bd_spacing">
  <view class="top">
    <block wx:for="{{detailList}}">
      <view class='title'> {{item.title}}</view>
      <view> {{item.detail}} </view>
    </block>
    <view class='bottom'>
      <button>马上完成</button>
      <button class='gray'>暂时接收</button>
    </view>
  </view>
</view>