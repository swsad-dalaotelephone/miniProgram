

.buttonLine{
    position: relative;
    margin-top: 40rpx;
    flex-direction: row;
    justify-content: space-between;
}


.buttonLine .no{
    background-color: #BED3FE;
}

.buttonLine .yes{
    background-color: #77A9FD;
}

.hidden{
    display:none;
}

.questionTitle {
  color: rgb(119, 169, 253);
}

.parent {
  width: 100%;
  /* height: 150px; */
  display: flex;
  flex-flow: row wrap;
  align-content: flex-start;
}

.child {
  box-sizing: border-box;
  background-color: white;
  flex: 0 0 50%;
  height: 30px; 
  /* border: 1px solid red; */
  display: flex;
}
.textPanel {
  width: 620rpx;
  margin: 20rpx 30rpx;
  border: solid 1px #BBBBBB;
  border-radius: 20rpx;
  text-align: start;
  padding: 5rpx;
  color: rgb(88, 96, 105);
}