<!--pages/register/register.wxml-->

<view class="titleBox">
  <view>SIGN UP</view>
</view>

<view class="inputPhone inputBox">
  <image class="headImg"/>
  <view class="text">Enter phone number</view>
</view>

<view class="inputPass inputBox">
  <image class="headImg"/>
  <view class="text">Password</view>
</view>

<view class="inputConfirmPass inputBox">
  <image class="headImg"/>
  <view class="text">Confirm password</view>
</view>

<view class="inputVeriCode inputBox">
  <image class="headImg"/>
  <view class="text">Verication code</view>
  <text class="obtainVeriCode" id="obtainVeriCode">Obtain</text>
</view>

<button class="confirmButton">CONFIRM</button>

<view class="toLogin"><text id="toLogin">登陆/Sign in</text></view>