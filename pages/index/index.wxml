<!--index.wxml-->
<view class='page'>
        <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" circular="{{circular}}" vertical="{{vertical}}" interval="{{interval}}" duration="{{duration}}" previous-margin="{{previousMargin}}px" next-margin="{{nextMargin}}px">
          <block wx:for="{{background}}" wx:key="*this">
            <swiper-item>
              <image class='imgw' src="{{item}}" mode="aspectFit" />
            </swiper-item>
          </block>
        </swiper>
  <view class="page__bd page__bd_spacing">
    <view class="weui-flex">
      <view class="weui-flex__item">
      </view>
    </view>
    <view class="weui-flex">
      <view class="weui-flex__item">
        <view class="placeholder">问卷</view>
      </view>
      <view class="weui-flex__item">
        <view class="placeholder">信息收集</view>
      </view>
      <view class="weui-flex__item">
        <view class="placeholder">招募</view>
      </view>
      <view class="weui-flex__item">
        <view class="placeholder">物流</view>
      </view>
    </view>
    <view class="weui-flex">
      <view class="weui-flex__item">
        <view class="placeholder">这是列表</view>
      </view>
    </view>
  </view>
</view>