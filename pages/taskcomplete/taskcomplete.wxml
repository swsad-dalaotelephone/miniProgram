<!--pages/taskcomplete/taskcomplete.wxml-->

  <view class='page'>
    <view class='head'>
      <i-icon type="return" bindtap='handleReturn' size="28" color="white"></i-icon>
      <text>{{page_title}}</text>
      <i-icon type="more" size="28" color="white"></i-icon>
    </view>

    <view class="page__bd page__bd_spacing pagebd" >
        <view class="newTaskPanel">  
            <view class="finishPanel">
              <view class="ok-icon">
                <i-icon type="success" size='80'/>
              </view>
              <view class="description">
                <view>
                  恭喜你完成了本次任务!
                </view>              
                <view>
                  请等待审核~
                </view>
              </view>
            </view>
            <view class='bottom'>
              <button bindtap="handleReturn">返回</button>
            </view>
        </view>
    </view>
  </view>


